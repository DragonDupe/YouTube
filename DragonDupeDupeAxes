  end
end
  if State == false then
    _G.d = false
  end
end)
tab2:Toggle("Always Night","Your World Will Always Night", false, function(State)
  if State == true then
    if _G.d == false then
    game.Lighting.TimeOfDay = "24:00:00"
    game.Lighting.Brightness = 0
    _G.n = State
  end
end
  if State == false then
      _G.n = false
  end
end)

tab2:Toggle("Full Bright","Full Bright Your World", false, function(State)
  if State == true then
    solara.fb = true
  end
  if State == false then
    solara.fb = false
  end
end)

tab2:Toggle("X Ray","Superman Mode", false, function(t)
  if t == true then
  for i, d in pairs(solara.workspc:GetDescendants()) do
		if d:IsA("BasePart") then
			local transpVal = Instance.new("NumberValue", d)
			transpVal.Name = "transpVal"
			transpVal.Value = d.Transparency
			d.Transparency = d.Transparency + 0.75
		end
	end
end
if t == false then
  for i, d in pairs(solara.workspc:GetDescendants()) do
		if d:IsA("BasePart") and d:FindFirstChild("transpVal") then
			d.Transparency = d:FindFirstChild("transpVal").Value
			d:FindFirstChild("transpVal"):Destroy()
		end
end
end
end)

