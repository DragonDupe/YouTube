--pengambilan ui libs-----------------------------
local DragonDupeWin = loadstring(game:HttpGet"https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/fluxlib.txt")()
--pembuatan judul gui--------------------------------
local DragonDupe = DragonDupeWin:Window("Dragon Dupe", "Easy Way To Build Your Base", Color3.fromRGB(255, 110, 48), Enum.KeyCode.RightShift)
--lokal semua gui------------------------------------
local solara = {}
solara.workspc = game:GetService("Workspace")
solara.runserv = game:GetService("RunService")
solara.light = game:GetService("Lighting")
solara.cgui = game:GetService("CoreGui")
solara.sgui = game:GetService("StarterGui")
solara.uis = game:GetService("UserInputService")
solara.teleserv = game:GetService("TeleportService")
solara.proxpromt= game:GetService("ProximityPromptService")
solara.replstr = game:GetService("ReplicatedStorage")
solara.plrs = game:GetService("Players")
solara.plr = solara.plrs.LocalPlayer
solara.chr = solara.plr.Character
solara.hum = solara.chr.Humanoid
solara.hrp = solara.chr.HumanoidRootPart
solara.camera = solara.workspc.CurrentCamera
solara.Head = solara.chr.Head
solara.Ferry = solara.workspc.Ferry
solara.hidewater = 0
solara.mouse = solara.plr:GetMouse()
solara.fb = true
solara.nc = false
solara.larikencang = 40
solara.terbang = true
solara.dupeaxe = ""


--FULL BRIGHT FUNC--------------------------------
if not solara.fb then

	_G.FullBrightEnabled = false

	_G.NormalLightingSettings = {
		Brightness = solara.light.Brightness,
		ClockTime = solara.light.ClockTime,
		FogEnd = solara.light.FogEnd,
		GlobalShadows = solara.light.GlobalShadows,
		Ambient = solara.light.Ambient
	}

	solara.light:GetPropertyChangedSignal("Brightness"):Connect(function()
		if solara.light.Brightness ~= 1 and solara.light.Brightness ~= _G.NormalLightingSettings.Brightness then
			_G.NormalLightingSettings.Brightness = solara.light.Brightness
			if not _G.FullBrightEnabled then
				repeat
					wait()
				until _G.FullBrightEnabled
			end
			solara.light.Brightness = 1
		end
	end)

	solara.light:GetPropertyChangedSignal("ClockTime"):Connect(function()
		if solara.light.ClockTime ~= 12 and solara.light.ClockTime ~= _G.NormalLightingSettings.ClockTime then
			_G.NormalLightingSettings.ClockTime = solara.light.ClockTime
			if not _G.FullBrightEnabled then
				repeat
					wait()
				until _G.FullBrightEnabled
			end
			solara.light.ClockTime = 12
		end
	end)

	solara.light:GetPropertyChangedSignal("FogEnd"):Connect(function()
		if solara.light.FogEnd ~= 786543 and solara.light.FogEnd ~= _G.NormalLightingSettings.FogEnd then
			_G.NormalLightingSettings.FogEnd = solara.light.FogEnd
			if not _G.FullBrightEnabled then
				repeat
					wait()
				until _G.FullBrightEnabled
			end
			solara.light.FogEnd = 786543
		end
	end)

	solara.light:GetPropertyChangedSignal("GlobalShadows"):Connect(function()
		if solara.light.GlobalShadows ~= false and solara.light.GlobalShadows ~= _G.NormalLightingSettings.GlobalShadows then
			_G.NormalLightingSettings.GlobalShadows = solara.light.GlobalShadows
			if not _G.FullBrightEnabled then
				repeat
					wait()
				until _G.FullBrightEnabled
			end
			solara.light.GlobalShadows = false
		end
	end)

	solara.light:GetPropertyChangedSignal("Ambient"):Connect(function()
		if solara.light.Ambient ~= Color3.fromRGB(178, 178, 178) and solara.light.Ambient ~= _G.NormalLightingSettings.Ambient then
			_G.NormalLightingSettings.Ambient = solara.light.Ambient
			if not _G.FullBrightEnabled then
				repeat
					wait()
				until _G.FullBrightEnabled
			end
			solara.light.Ambient = Color3.fromRGB(178, 178, 178)
		end
	end)

	solara.light.Brightness = 1
	solara.light.ClockTime = 12
	solara.light.FogEnd = 786543
	solara.light.GlobalShadows = false
	solara.light.Ambient = Color3.fromRGB(178, 178, 178)

	local LatestValue = true
	spawn(function()
		repeat
			wait()
		until _G.FullBrightEnabled
		while wait() do
			if _G.FullBrightEnabled ~= LatestValue then
				if not _G.FullBrightEnabled then
					solara.light.Brightness = _G.NormalLightingSettings.Brightness
					solara.light.ClockTime = _G.NormalLightingSettings.ClockTime
					solara.light.FogEnd = _G.NormalLightingSettings.FogEnd
					solara.light.GlobalShadows = _G.NormalLightingSettings.GlobalShadows
					solara.light.Ambient = _G.NormalLightingSettings.Ambient
				else
					solara.light.Brightness = 1
					solara.light.ClockTime = 12
					solara.light.FogEnd = 786543
					solara.light.GlobalShadows = false
					solara.light.Ambient = Color3.fromRGB(178, 178, 178)
				end
				LatestValue = not LatestValue
			end
		end
	end)
end


_G.FullBrightEnabled = not _G.FullBrightEnabled
--Tes Dulu-------------------------------------------------------
_G.rf = false
_G.n = false
_G.d = false

game.Lighting.Changed:connect(function()
  if _G.d then
    game.Lighting.TimeOfDay = "12:00:00"
    game.Lighting.Brightness = 1
  end
  if _G.n then
    game.Lighting.TimeOfDay = "24:00:00"
    game.Lighting.Brightness = 0
  end
  if _G.rf then
    game.Lighting.FogEnd = 9999

  end
  end)
