--(Script Dibuka)
solara.sgui:SetCore("SendNotification", {
    Title = "Dragon Dupe";
    Text = "Welcome "..solara.plr.Name
})
--(Script Dimatikan)
solara.cgui.ChildRemoved:Connect(function(child)
    if child.Name == "FluxLib" then
      if  InfiniteJumpEnabled == true then  InfiniteJumpEnabled = false end
      if flying == true then flying = false end
      if solara.terbang == true then solara.terbang = false end
      if _G.d == true then _G.d = false end
      if _G.n == true then _G.n = false end
      if solara.nc == true then solara.nc = false end
      solara.hum.JumpPower = 50
      for _,v in pairs(solara.workspc.Properties:GetChildren()) do
        if v:FindFirstChild("Owner") and v.Owner.Value == game.Players.LocalPlayer then
          for _,k in pairs(v:GetChildren()) do
            if k:IsA("Part") then
              k.Color = Color3.fromRGB(124, 92, 70)
            end
          end
        end
      end
      for i, v in pairs(solara.workspc.Water:GetChildren()) do
        if v.Name == "Water" then v.CanCollide = false end
      end
      for i, d in pairs(solara.workspc:GetDescendants()) do
        if d:IsA("BasePart") and d:FindFirstChild("transpVal") then
          d.Transparency = d:FindFirstChild("transpVal").Value
          d:FindFirstChild("transpVal"):Destroy()
        end
    end
    solara.hum.HipHeight = 0
      lari:Disconnect()
      CLICKTP:Disconnect()
      solara.fb = false

      solara.sgui:SetCore("SendNotification", {
        Title = "Dragon Dupe";
        Text = "Subscribe Dragon Dupe "..game.Players.LocalPlayer.Name
    })
  end
end)

