--[Fly Function]-------------------------------------------------------
repeat wait()
until solara.plr and solara.chr and solara.chr:FindFirstChild("Torso") and solara.hum

repeat wait() until solara.mouse

local torso = solara.chr.Torso
local flying = false
local deb = true
local ctrl = {f = 0, b = 0, l = 0, r = 0}
local lastctrl = {f = 0, b = 0, l = 0, r = 0}
local maxspeed = 10000
local speed = 5
local Flyspeed = 25

function Fly()
    bg = Instance.new("BodyGyro", torso)
    bg.P = 9e4
    bg.maxTorque = Vector3.new(9e9, 9e9, 9e9)
    bg.cframe = torso.CFrame
    bv = Instance.new("BodyVelocity", torso)
    bv.velocity = Vector3.new(0,0.1,0)
    bv.maxForce = Vector3.new(9e9, 9e9, 9e9)
    repeat wait()
      solara.hum.PlatformStand = true
      if ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0 then
        speed = 5
        if speed > maxspeed then
          speed = maxspeed
        end
      elseif not (ctrl.l + ctrl.r ~= 0 or ctrl.f + ctrl.b ~= 0) and speed ~= 0 then
        speed = speed-1
        if speed < 0 then
          speed = 0
        end
      end
      if (ctrl.l + ctrl.r) ~= 0 or (ctrl.f + ctrl.b) ~= 0 then
        bv.velocity = ((solara.workspc.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f+ctrl.b)) + ((solara.workspc.CurrentCamera.CoordinateFrame * CFrame.new(ctrl.l+ctrl.r,(ctrl.f+ctrl.b)*.2,0).p) - solara.workspc.CurrentCamera.CoordinateFrame.p))*speed
        lastctrl = {f = ctrl.f, b = ctrl.b, l = ctrl.l, r = ctrl.r}
      elseif (ctrl.l + ctrl.r) == 0 and (ctrl.f + ctrl.b) == 0 and speed ~= 0 then
        bv.velocity = ((solara.workspc.CurrentCamera.CoordinateFrame.lookVector * (lastctrl.f+lastctrl.b)) + ((solara.workspc.CurrentCamera.CoordinateFrame * CFrame.new(lastctrl.l+lastctrl.r,(lastctrl.f+lastctrl.b)*.2,0).p) - solara.workspc.CurrentCamera.CoordinateFrame.p))*speed
      else
        bv.velocity = Vector3.new(0,0.1,0)
      end
      bg.cframe = solara.workspc.CurrentCamera.CoordinateFrame * CFrame.Angles(-math.rad((ctrl.f+ctrl.b)*50*speed/maxspeed),0,0)
    until not flying
    ctrl = {f = 0, b = 0, l = 0, r = 0}
    lastctrl = {f = 0, b = 0, l = 0, r = 0}
    speed = 0
    bg:Destroy()
	bg = nil
    bv:Destroy()
	bv = nil
    solara.hum.PlatformStand = false
end

solara.mouse.KeyDown:connect(function(key)
	if key:lower() == "" then
		if flying then 
			flying = false
		else
			flying = true
			Fly()
		end
	elseif key:lower() == "w" then
		ctrl.f = Flyspeed
	elseif key:lower() == "s" then
		ctrl.b = -Flyspeed
	elseif key:lower() == "a" then
		ctrl.l = -Flyspeed
	elseif key:lower() == "d" then
		ctrl.r = Flyspeed
	end
end)

solara.mouse.KeyUp:connect(function(key)
	if key:lower() == "w" then
		ctrl.f = 0
	elseif key:lower() == "s" then
		ctrl.b = 0
	elseif key:lower() == "a" then
		ctrl.l = 0
	elseif key:lower() == "d" then
		ctrl.r = 0
	end
end)
solara.hum.StateChanged:Connect(function(dragon,dupe)
    if dupe == Enum.HumanoidStateType.Running then
        ctrl.f = -1
    else
        ctrl.f = 0
    end

end)
Fly()

tab1:Toggle("Flying", "Terbang Seperti Superman", true, function(ggg)
  if ggg == true then
    solara.terbang = true
  end
  if ggg == false then
    flying = false
    solara.terbang = false
  end
end)
tab1:Bind("Fly KeyBind", Enum.KeyCode.Q, function()
  if solara.terbang == true then
    if flying then flying = false
else
    flying = true
    Fly()
end
end 
end)

tab1:Slider("Fly Speed", "Makes Your Fly Speed", 0, 200, 25, function(angka)
Flyspeed = angka
end)

