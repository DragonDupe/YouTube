  end)
tab1:Button("BTools","telor" ,function()
  local movetool = Instance.new("Tool", solara.plr.Backpack)
    local deletetool = Instance.new("Tool", solara.plr.Backpack)
    local undotool = Instance.new("Tool", solara.plr.Backpack)
    local identifytool = Instance.new("Tool", solara.plr.Backpack)
    local movedetect = false
    local movingpart = nil
    local movetransparency = 0
        if editedparts == nil then
        editedparts = {}
        parentfix = {}
        positionfix = {}
        end
    deletetool.Name = "Delete"
    undotool.Name = "Undo"
    identifytool.Name = "Identify"
    movetool.Name = "Move"
    undotool.CanBeDropped = false
    deletetool.CanBeDropped = false
    identifytool.CanBeDropped = false
    movetool.CanBeDropped = false
    undotool.RequiresHandle = false
    deletetool.RequiresHandle = false
    identifytool.RequiresHandle = false
    movetool.RequiresHandle = false
    local function createnotification(title, text)
        game:GetService("StarterGui"):SetCore("SendNotification", {
        Title = title;
        Text = text;
        Duration = 1;
        })
    end
        deletetool.Activated:Connect(function()
        createnotification("Dragon Dupe", "You have deleted "..solara.mouse.Target.Name)
        table.insert(editedparts, solara.mouse.Target)
        table.insert(parentfix, solara.mouse.Target.Parent)
        table.insert(positionfix, solara.mouse.Target.CFrame)
        solara.mouse.Target.Parent = nil
        end)
        undotool.Activated:Connect(function()
        createnotification("Dragon Dupe", "You have undone "..editedparts[#editedparts].Name)
        editedparts[#editedparts].Parent = parentfix[#parentfix]
        editedparts[#editedparts].CFrame = positionfix[#positionfix]
        table.remove(positionfix, #positionfix)
        table.remove(editedparts, #editedparts)
        table.remove(parentfix, #parentfix)
        end)
        identifytool.Activated:Connect(function()
        createnotification("Dragon Dupe", "Instance: "..solara.mouse.Target.ClassName.."\nName: "..solara.mouse.Target.Name)
        end)
        movetool.Activated:Connect(function()
        createnotification("Dragon Dupe", "You are moving: "..solara.mouse.Target.Name)
        movingpart = solara.mouse.Target
        movedetect = true
        movingpart.CanCollide = false
        movetransparency = movingpart.Transparency
        movingpart.Transparency = 0.5
        solara.mouse.TargetFilter = movingpart
        table.insert(editedparts, movingpart)
        table.insert(parentfix, movingpart.Parent)
        table.insert(positionfix, movingpart.CFrame)
        movingpart.Transparency = movingpart.Transparency / 2
        repeat
        solara.mouse.Move:Wait()
        movingpart.CFrame = CFrame.new(solara.mouse.Hit.p)
        until movedetect == false
        end)
        movetool.Deactivated:Connect(function()
        createnotification("Dragon Dupe", "You have stopped moving: "..solara.mouse.Target.Name)
        movingpart.CanCollide = true
        movedetect = false
        solara.mouse.TargetFilter = nil
        movingpart.Transparency = movetransparenc
        end)
end)
tab1:Slider("Zoom Distance", "Makes your Zoom", 0, 1000,200, function(Value)
  solara.plr.CameraMaxZoomDistance = Value
  end)
tab1:Line()
tab1:Toggle("Hard Dragger", "Drag Any Items", false, function(t)
  if t == true then
    local dragongrabs = true
    HardDragonGrab = solara.workspc.ChildAdded:Connect(function(dragongrab)
      if dragongrab.Name == "Dragger" then
          local dragongrabbody = dragongrab:WaitForChild("BodyGyro")
          local dragongrabposition = dragongrab:WaitForChild("BodyPosition")
          repeat
              wait()
              dragongrabposition.P = 120000;
              dragongrabposition.D = 1000;
              dragongrabposition.maxForce = Vector3.new(1, 1, 1) * 1000000;
              dragongrabbody.maxTorque = Vector3.new(1, 1, 1) * 200;
              dragongrabbody.P = 1200;
              dragongrabbody.D = 140
          until not dragongrab
      end
    end)
    else HardDragonGrab:Disconnect()
  end
if t == false then
  dragongrabs = false
end
end)
tab1:Toggle("No Clip", "No Clip Like A Ghost", false, function(t)
    if t == true then
      solara.nc = true
      Stepped = game:GetService("RunService").Stepped:Connect(function()
        if not solara.nc == false then
          for a, b in pairs(solara.workspc:GetChildren()) do
                  if b.Name == solara.plr.Name then
                  for i, v in pairs(solara.workspc[solara.plr.Name]:GetChildren()) do
                  if v:IsA("BasePart") then
                  v.CanCollide = false
                  end end end end
        else
          Stepped:Disconnect()
        end
      end)
    elseif t == false then
      if solara.nc == true then solara.nc = false 
    end
  end
  end)
tab1:Toggle("Walk On Water", "Now You Can Walk On Water", false, function(walwater)
if walwater == true then
  for i, v in pairs(solara.workspc.Water:GetChildren()) do
    if v.Name == "Water" then v.CanCollide = walwater end
end
end
if walwater == false then
  for i, v in pairs(solara.workspc.Water:GetChildren()) do
    if v.Name == "Water" then v.CanCollide = walwater end
end
end
end)


